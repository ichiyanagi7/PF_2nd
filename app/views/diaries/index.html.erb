<div class="container">
  <div class="row mb-5">
    <h3>マイプレイリスト</h3>
  </div>

  <div class="row">
    <% @diaries.each do |key,diaries| %>
    <div class="col-12 mb-3 border-bottom">
      <h3><%= key[0,4] + "年" + key[4,2] + "月" %></h3>
      <div class="table">
        <table>
          <thead>
            <th>アーティスト</th>
            <th>曲名</th>
            <th></th>
          </thead>
        <% diaries.each do |diary| %>
          <tbody>
            <td><%= diary.artist %></td>
            <td><%= diary.music %></td>
          </tbody>
        <% end %>
        </table>
        </div>
      </div>
      <% end %>
  </div>

</div>